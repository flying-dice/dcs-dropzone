{
	"name": "launcher",
	"private": true,
	"type": "module",
	"scripts": {
		"biome": "biome check --write",
		"build": "bun build.ts",
		"depcheck": "bunx depcheck",
		"dev": "RELEASE_BASEURL='http://localhost:8081/' bun --watch src/index.ts",
		"tests": "bun test --coverage",
		"tsc": "tsc --noEmit"
	},
	"dependencies": {
		"@packages/manifest": "workspace:*"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.0",
		"@types/bun": "1.3.6"
	}
}
