{
	"name": "bun-react-template",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"biome": "biome check --write",
		"tsc": "tsc --noEmit",
		"check": "bun run biome && bun run tsc",
		"dev": "bun --hot src/app/index.ts",
		"dev:daemon": "bun --hot src/daemon/index.ts",
		"test": "bun test",
		"clientgen": "bun run --bun orval --config orval.config.cjs",
		"build": "bun build ./src/app/index.ts --minify --sourcemap --compile --env='BUN_PUBLIC_*' --define:process.env.NODE_ENV='\"production\"' --outfile ./dist/app",
		"build:daemon": "bun scripts/build-daemon.ts",
		"start": "docker compose up",
		"drizzle": "drizzle-kit generate --config drizzle.config.ts --name=init",
		"postdrizzle": "bun drizzle-migration.setup.ts"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@fontsource/inter": "^5.2.8",
		"@hono/standard-validator": "^0.1.5",
		"@mantine/colors-generator": "^8.3.5",
		"@mantine/core": "^8.3.6",
		"@mantine/dates": "^8.3.5",
		"@mantine/form": "^8.3.5",
		"@mantine/hooks": "^8.3.6",
		"@mantine/modals": "^8.3.5",
		"@mantine/notifications": "^8.3.5",
		"@mantine/spotlight": "^8.3.6",
		"@monaco-editor/react": "^4.7.0",
		"@scalar/hono-api-reference": "^0.9.22",
		"@tanstack/react-query": "^5.90.5",
		"@types/getenv": "^1.0.3",
		"@types/ms": "^2.1.0",
		"@types/object-hash": "^3.0.6",
		"axios": "^1.12.2",
		"cloudflare": "^5.2.0",
		"date-fns": "^4.1.0",
		"drizzle-kit": "^0.31.6",
		"drizzle-orm": "^0.44.7",
		"fs-extra": "^11.3.2",
		"getenv": "^2.0.0",
		"hono": "^4.10.3",
		"hono-openapi": "^1.1.0",
		"http-status-codes": "^2.3.0",
		"i18next": "^25.6.2",
		"i18next-browser-languagedetector": "^8.2.0",
		"i18next-http-backend": "^3.0.2",
		"mantine-form-zod-resolver": "^1.3.0",
		"marked": "^16.4.1",
		"monaco-editor": "^0.54.0",
		"mongodb": "^6.20.0",
		"mongodb-memory-server": "^10.3.0",
		"mongoose": "^8.19.3",
		"ms": "^2.1.3",
		"object-hash": "^3.0.0",
		"octokit": "^5.0.4",
		"orval": "^8.0.0-rc.0",
		"pino": "^10.1.0",
		"pino-http": "^11.0.0",
		"pino-pretty": "^13.1.2",
		"react": "^19",
		"react-dom": "^19",
		"react-i18next": "^16.3.3",
		"react-icons": "^5.5.0",
		"react-query": "^3.39.3",
		"react-router-dom": "^7.9.4",
		"react-use": "^17.6.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.0",
		"@types/bun": "latest",
		"@types/fs-extra": "^11.0.4",
		"@types/marked": "^6.0.0",
		"@types/react": "^19",
		"@types/react-dom": "^19"
	}
}
