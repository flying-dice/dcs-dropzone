{
	"name": "bun-react-template",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"biome": "biome check --write",
		"dev": "bun --hot src/index.ts",
		"orval": "orval --config orval.config.cjs",
		"build": "bun build ./src/index.tsx --minify --sourcemap --compile --env='BUN_PUBLIC_*' --define:process.env.NODE_ENV='\"production\"' --outfile ./dist/app",
		"start": "docker compose up"
	},
	"dependencies": {
		"@hono/standard-validator": "^0.1.5",
		"@mantine/core": "^8.3.5",
		"@mantine/dates": "^8.3.5",
		"@mantine/form": "^8.3.5",
		"@mantine/hooks": "^8.3.5",
		"@mantine/modals": "^8.3.5",
		"@mantine/notifications": "^8.3.5",
		"@scalar/hono-api-reference": "^0.9.22",
		"@types/getenv": "^1.0.3",
		"@types/ms": "^2.1.0",
		"axios": "^1.12.2",
		"cloudflare": "^5.2.0",
		"getenv": "^2.0.0",
		"hono": "^4.10.3",
		"hono-openapi": "^1.1.0",
		"http-status-codes": "^2.3.0",
		"jose": "^6.1.0",
		"mongodb": "^6.20.0",
		"ms": "^2.1.3",
		"octokit": "^5.0.4",
		"orval": "^7.14.0",
		"pino": "^10.1.0",
		"pino-http": "^11.0.0",
		"react": "^19",
		"react-dom": "^19",
		"react-icons": "^5.5.0",
		"react-query": "^3.39.3",
		"react-router-dom": "^7.9.4",
		"react-use": "^17.6.0",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.0",
		"@types/bun": "latest",
		"@types/react": "^19",
		"@types/react-dom": "^19"
	}
}
