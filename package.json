{
	"name": "dcs-dropzone",
	"version": "0.1.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"check": "bun run --workspaces check",
		"tests": "bun run --workspaces tests",
		"dev:webapp": "cd apps/webapp && bun run dev",
